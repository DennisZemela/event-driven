<application>
<name>ED Gauss Clustering </name>

<dependencies>	
</dependencies>

<module>
   <name>dataSetPlayer</name>
   <node>localhost</node>
   <tag>dataSetPlayer</tag>
</module>

<module>
   <name>eventBottleConverter</name>
   <node>localhost</node>
   <tag>eventBottleConvert</tag>
</module>

<module>
    <name>eventClustering</name>
    <node>localhost</node>
    <tag>eventClustering</tag>
</module>

<module>
   <name>yarpview</name>
   <parameters>--name /eventViewLeft --x 000 --y 000 --RefreshTime 50 </parameters>
   <node>localhost</node>
   <tag>eventViewerLeft</tag>
</module>
<module>
   <name>yarpview</name>
   <parameters>--name /eventViewRight --x 350 --y 000 --RefreshTime 50 </parameters>
   <node>localhost</node>
   <tag>eventViewerRight</tag>
</module>


<connection>
  <from>/eventBottlePlayer/etb:o</from>
  <to>/eventBottleConverter/bte:i</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/eventBottleConverter/bte:o</from>
  <to>/eventClustering/eventBottle:i</to>
  <protocol>tcp</protocol>
</connection>
<connection>
    <from>/eventClustering/leftImage:o</from>
      <to>/eventViewLeft</to>
  <protocol>tcp</protocol>
</connection>
<connection>
    <from>/eventClustering/rightImage:o</from>
    <to>/eventViewRight</to>
    <protocol>tcp</protocol>
</connection>

</application>
