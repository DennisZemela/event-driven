<application>
<name>ED Gauss Clustering </name>

<dependencies>	
</dependencies>

<module>
    <name>eventClustering</name>
    <node>localhost</node>
    <tag>eventClustering</tag>
</module>

<module>
   <name>yarpview</name>
   <parameters>--name /eventViewLeft --x 000 --y 000 --RefreshTime 50 </parameters>
   <node>localhost</node>
   <tag>eventViewerLeft</tag>
</module>
<module>
   <name>yarpview</name>
   <parameters>--name /eventViewRight --x 350 --y 000 --RefreshTime 50 </parameters>
   <node>localhost</node>
   <tag>eventViewerRight</tag>
</module>


<connection>
  <from>/cbottletoebottle/cte:o</from>
  <to>/eventClustering/vBottle:i</to>
  <protocol>tcp</protocol>
</connection>
<connection>
    <from>/eventClustering/leftImage:o</from>
      <to>/eventViewLeft</to>
  <protocol>tcp</protocol>
</connection>
<connection>
    <from>/eventClustering/rightImage:o</from>
    <to>/eventViewRight</to>
    <protocol>tcp</protocol>
</connection>

</application>
